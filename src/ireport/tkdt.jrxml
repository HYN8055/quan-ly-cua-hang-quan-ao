	<queryString language="SQL">
		<![CDATA[select
    COALESCE(DoanhThu, 0) AS DoanhThu, 
    COALESCE(Von, 0) AS Von, 
    COALESCE(DoanhThu, 0) - COALESCE(Von, 0) AS LoiNhuan, 
    COALESCE(ThangPN, ThangHD) AS Thang
from 
(select  sum(tongtien) AS DoanhThu, EXTRACT (MONTH FROM ngayin) AS ThangHD
from HOADON
group by EXTRACT (MONTH FROM ngayin)) full join
(select sum(tongtien) AS Von, EXTRACT (MONTH FROM ngaytao) AS ThangPN
from PHIEUNHAPHANG
group by EXTRACT (MONTH FROM ngaytao))
on ThangHD=ThangPN
order by ThangHD desc]]>
	</queryString>
