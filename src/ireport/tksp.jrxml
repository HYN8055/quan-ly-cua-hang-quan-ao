	<queryString language="SQL">
		<![CDATA[SELECT t1.masp, tensp ,slNhap,slXuat FROM (
SELECT masp, SUM(soluong) AS slNhap FROM CTNHAP 
JOIN PHIEUNHAPHANG ON PHIEUNHAPHANG.mapnh = CTNHAP.mapnh
GROUP BY masp) t1 
JOIN (
SELECT masp, SUM(soluong) AS slXuat FROM CTHD 
JOIN HOADON ON HOADON.mahd = CTHD.mahd 
GROUP BY masp ) t2
ON t1.masp = t2.masp
JOIN SANPHAM ON t1.masp = SANPHAM.masp]]>
	</queryString>
